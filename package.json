{
  "name": "fastify-play",
  "version": "1.0.0",
  "description": "fastify-play",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node --transpile-only ./src/index.ts",
    "seed": "ts-node --transpile-only ./prisma/seed.ts",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma reset --force",
    "build:sandbox": "esbuild apollo/load.js --bundle --outfile=public/apollo/load.js",
    "lint": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --debug --ext \".ts\" --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/W-David/fastify-play.git"
  },
  "author": "Coody",
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 18.0.0"
  },
  "devDependencies": {
    "@apollo/sandbox": "^2.6.0",
    "@fastify/static": "^5.0.0",
    "@types/graphql-fields": "^1.3.9",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "0.21.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.7.0",
    "typegraphql-prisma": "^0.27.2",
    "typescript": "^4.6.2"
