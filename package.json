    "@as-integrations/fastify": "^2.1.1",
    "@prisma/client": "^5.15.1",
    "chalk": "^4.1.0",
    "class-validator": ">=0.12.0",
    "fastify": "^4.28.0",
    "fastify-static": "^4.5.0",
    "graphql": "^16.8.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.23.0",
    "mercurius": "^14.1.0",
    "prisma": "^5.15.1",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.6.3",
    "type-graphql": "^2.0.0-rc.1"
  }
